{
  "name": "@astronautlabs/rtp",
  "version": "0.0.1",
  "description": "An RTP implementation for Node.js and the browser ",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "doc": "doc",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "cpp:configure": "cmake -A x64 -S . -B build",
