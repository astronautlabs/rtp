    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astronautlabs/rtp.git"
  },
  "keywords": [
    "rtp",
    "network",
    "media",
    "sdp"
  ],
